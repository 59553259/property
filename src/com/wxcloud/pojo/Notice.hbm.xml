<?xml version="1.0" encoding="utf-8"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">  
<hibernate-mapping>  
	<class name="com.wxcloud.pojo.Notice" table="noticemgt">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="InfoTitle" type="java.lang.String">
			<column name="InfoTitle" length="32"></column>
		</property>
		<property name="InfoContent" type="java.lang.String">
			<column name="InfoContent" length="128"></column>
		</property>
		<property name="InfoTime" type="java.util.Date">
			<column name="InfoTime"/>
		</property>
	</class>
</hibernate-mapping>